{
  "presets": [
    ["env", {
      "targets": {
        "browsers": ["last 2 versions", "IE >= 11"]
      },
      "start": {
        "presets": [
        ]
      }
    }],
    ["es2015", {"modules": false}],
    "react",
    "stage-0"
  ],
  "plugins": [
    "syntax-dynamic-import", 
    "transform-object-rest-spread", 
    "transform-decorators-legacy",
    ["module-resolver", {
      "root": ["./src"],
      "alias": {
        "zoapp-front": "./shared"
      }
    }],
    ["transform-runtime", {
      "polyfill": false,
      "regenerator": true
    }]
  ],
  "env": {
    "test": {
      "presets": [
        ["env", {
          "targets": {
            "node": 8
          },
          "modules": false
        }],
        "es2017",
        "react",
        "stage-0"
      ]
    }
  }
}
