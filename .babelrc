{
    "plugins": [
        "syntax-dynamic-import",
        "transform-class-properties",
        "transform-object-rest-spread",
        "transform-decorators-legacy",
        "react-hot-loader/babel",
        [
            "module-resolver",
            {
                "alias": {
                    "zoapp-front": "./shared"
                },
                "root": [
                    "./src"
                ]
            }
        ],
        [
            "transform-runtime",
            {
                "polyfill": false,
                "regenerator": true
            }
        ]
    ],
    "presets": [
        [
            "env",
            {
                "targets": {
                    "browsers": [
                        "last 2 versions",
                        "IE >= 11"
                    ]
                }
            }
        ],
        "react"
    ]
}
